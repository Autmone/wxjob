<!--index.wxml-->
<view>
    <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item.url}}" class="slide-image" width="100%" height="150" bindtap="{{item.function}}" />
    </swiper-item>
  </block>
</swiper>
</view>

